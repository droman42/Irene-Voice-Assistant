          {
            "LOWER": "на"
          },
          {
            "LIKE_NUM": true
          }
        ],
        [
          {
            "TEXT": {
              "REGEX": "set|start"
            }
          },
          {
            "TEXT": {
              "REGEX": "timer|alarm"
            }
          }
        ]
      ],
      "slot_patterns": {
        "DURATION_VALUE": [
          [
            {
              "LIKE_NUM": true
            },
            {
              "LEMMA": {
                "IN": [
                  "минута",
                  "секунда",
                  "час"
