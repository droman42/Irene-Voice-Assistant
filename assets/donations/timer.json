        "TIME_UNIT": [
          [
            {
              "LEMMA": {
                "IN": [
                  "секунда",
                  "минута",
                  "час"
                ]
              }
            }
          ],
          [
            {
              "LOWER": {
                "IN": [
                  "сек",
                  "мин",
                  "ч",
                  "seconds",
                  "minutes",
                  "hours"
                ]
              }
            }
          ]
        ],
        "TIMER_MESSAGE": [
          [
            {
              "LOWER": "сообщение"
            },
            {
              "IS_ALPHA": true,
              "OP": "+"
            }
          ],
          [
            {
              "LOWER": "напомни"
            },
            {
              "IS_ALPHA": true,
              "OP": "+"
            }
          ]
        ]
      },
      "examples": [
        {
          "text": "поставь таймер на 5 минут",
          "parameters": {
            "duration": 5,
            "unit": "minutes"
          }
        },
        {
          "text": "заведи будильник на 2 часа с сообщением проснись",
